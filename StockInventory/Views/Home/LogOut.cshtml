@model LoggedIn
@{
    ViewData["Title"] = "LogOut";
    Layout = "~/Views/Shared/_LayoutLIn.cshtml";
}
@section one{}

<html>
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
  <link rel="stylesheet" href="~/css/user.css" />
  <title>Sign Up</title>
</head>
<body>
  <div class="mainBox">
      <div class="darkFilter"></div>
          <div class="mainBord">
            <h2 style="color: white;">Log Out</h2>
            <form action="/Home/LogOut/@Model.LoggedInId" method="post">
                <input type="hidden" name="LoggedInId" value="@Model.LoggedInId" />

                <div style="color: white;">Are you sure to log out?</div>
                <button type="submit" class="btn btn-danger">Log Out</button>
                <a class="btn btn-warning" href="/home/AccountSetting">Cancel</a>
            </form>
          </div>
  </div>
</body>
</html>

